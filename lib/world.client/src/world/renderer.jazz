;;;
;;;; Transparent-Renderer
;;;


(class Transparent-Renderer extends Model-Renderer
  
  
  (method override (render self)
    (unless (empty? self)
      (use program)
      ;; meshes / entities
      (draw-meshes self .7)
      (draw-entities self .7)
      ;; ghosts
      (glEnable GL_CULL_FACE)
      (glCullFace GL_FRONT)
      (glEnable GL_BLEND)
      (draw-ghosts self .2)
      (glDisable GL_BLEND)
      (glCullFace GL_BACK)
      (glDisable GL_CULL_FACE)
      (unuse program))))


