(definition protected environment-title-suffix
  (cond-expand
    (prod   "")
    (stage  " Stage")
    (stable " Stable")
    (triage " Triage")
    (test   " Test")
    (trial  " Trial")
    (else   " Devel")))


(definition protected environment-suffix
  (cond-expand
    (prod   "")
    (stage  " (stage)")
    (stable " (stable)")
    (triage " (triage)")
    (test   " (test)")
    (trial  " (trial)")
    (else   " (devel)")))


