(module world.debuggee.implementation jazz


(import (jazz.ui.window)
        (world.debug)
        (world.client.window))


(class World-Debuggee extends Object
  
  
  (method public (position-world rect)
    (execute-event
      (lambda ()
        (position-world-window rect))))
  
  
  (method public (restore-world)
    (execute-event
      (lambda ()
        (restore-world-window))))
  
  
  (method public (set-property property value)
    (set-target-property property value))))
