;;;==============
;;;  JazzScheme
;;;==============
;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                   name: client          description-file: {File :context ".package"}
    (<*>                                             tag-reference: {File :context "client.project"})
    (<*>                                             tag-reference: {File :context "showcase-immediate.project"})
    (<*>                                             tag-reference: {File :context "showcase-shader.project"})
    (<Project-Directory>                             directory: {Directory :context "src"}
      (<Project-Directory>                           directory: {Directory :context "src" "world"}
        (<Project-Directory>                         directory: {Directory :context "src" "world" "client"}
          (<Jazz-File>         name: bubbles         source: {File :context "src" "world" "client" "bubbles.jazz"})
          (<Jazz-File>         name: run             source: {File :context "src" "world" "client" "run.jazz"})
          (<Jazz-File>         name: snakes          source: {File :context "src" "world" "client" "snakes.jazz"})
          (<Jazz-File>         name: tree            source: {File :context "src" "world" "client" "tree.jazz"})
          (<Jazz-File>         name: window          source: {File :context "src" "world" "client" "window.jazz"})
          (<Jazz-File>         name: _client         source: {File :context "src" "world" "client" "_client.jazz"})
          (<Jazz-File>         name: configuration   source: {File :context "src" "world" "client" "configuration.jazz"})
          (<Project-Directory>                       directory: {Directory :context "src" "world" "client" "remote"}
            (<Jazz-File>       name: implementation  source: {File :context "src" "world" "client" "remote" "implementation.jazz"})
            (<Jazz-File>       name: _remote         source: {File :context "src" "world" "client" "remote" "_remote.jazz"})))
        (<Jazz-File>           name: _world          source: {File :context "src" "world" "_world.jazz"})
        (<Jazz-File>           name: texture         source: {File :context "src" "world" "texture.jazz"})
        (<Jazz-File>           name: window          source: {File :context "src" "world" "window.jazz"})
        (<Project-Directory>                         directory: {Directory :context "src" "world" "showcase"}
          (<Project-Directory>                       directory: {Directory :context "src" "world" "showcase" "immediate"}
            (<Jazz-File>       name: run             source: {File :context "src" "world" "showcase" "immediate" "run.jazz"})
            (<Jazz-File>       name: window          source: {File :context "src" "world" "showcase" "immediate" "window.jazz"}))
          (<Project-Directory>                       directory: {Directory :context "src" "world" "showcase" "shader"}
            (<Jazz-File>       name: run             source: {File :context "src" "world" "showcase" "shader" "run.jazz"})
            (<Jazz-File>       name: window          source: {File :context "src" "world" "showcase" "shader" "window.jazz"})))
        (<Jazz-File>           name: autoload        source: {File :context "src" "world" "autoload.jazz"})
        (<Project-Directory>                         directory: {Directory :context "src" "world" "validate"}
          (<Project-Directory>                       directory: {Directory :context "src" "world" "validate" "suite"}
            (<Jazz-File>       name: Geometry-Tester source: {File :context "src" "world" "validate" "suite" "Geometry-Tester.jazz"}))
          (<Jazz-File>         name: _validate       source: {File :context "src" "world" "validate" "_validate.jazz"})
          (<Jazz-File>         name: autoload        source: {File :context "src" "world" "validate" "autoload.jazz"})
          (<Jazz-File>         name: run             source: {File :context "src" "world" "validate" "run.jazz"}))
        (<Jazz-File>           name: history         source: {File :context "src" "world" "history.jazz"})
        (<Jazz-File>           name: counter         source: {File :context "src" "world" "counter.jazz"})
        (<Jazz-File>           name: folio           source: {File :context "src" "world" "folio.jazz"})
        (<Jazz-File>           name: settings        source: {File :context "src" "world" "settings.jazz"})
        (<Jazz-File>           name: profile         source: {File :context "src" "world" "profile.jazz"})
        (<Jazz-File>           name: test            source: {File :context "src" "world" "test.jazz"})
        (<Jazz-File>           name: draw            source: {File :context "src" "world" "draw.jazz"})
        (<Jazz-File>           name: mark            source: {File :context "src" "world" "mark.jazz"})
        (<Jazz-File>           name: render          source: {File :context "src" "world" "render.jazz"})
        (<Jazz-File>           name: shader          source: {File :context "src" "world" "shader.jazz"})
        (<Jazz-File>           name: light           source: {File :context "src" "world" "light.jazz"})
        (<Jazz-File>           name: product         source: {File :context "src" "world" "product.jazz"})
        (<Jazz-File>           name: eval            source: {File :context "src" "world" "eval.jazz"})
        (<Jazz-File>           name: evaluate        source: {File :context "src" "world" "evaluate.jazz"})
        (<Jazz-File>           name: shaders         source: {File :context "src" "world" "shaders.jazz"})
        (<Jazz-File>           name: io              source: {File :context "src" "world" "io.jazz"})
        (<Project-Directory>                         directory: {Directory :context "src" "world" "debuggee"}
          (<Jazz-File>         name: _debuggee       source: {File :context "src" "world" "debuggee" "_debuggee.jazz"})
          (<Jazz-File>         name: stub            source: {File :context "src" "world" "debuggee" "stub.jazz"})
          (<Jazz-File>         name: implementation  source: {File :context "src" "world" "debuggee" "implementation.jazz"}))
        (<Jazz-File>           name: debug           source: {File :context "src" "world" "debug.jazz"})
        (<Jazz-File>           name: array           source: {File :context "src" "world" "array.jazz"})
        (<Jazz-File>           name: pack            source: {File :context "src" "world" "pack.jazz"})
        (<Jazz-File>           name: face            source: {File :context "src" "world" "face.jazz"})
        (<Jazz-File>           name: triangle        source: {File :context "src" "world" "triangle.jazz"})
        (<Project-Directory>                         directory: {Directory :context "src" "world" "foreign"}
          (<Jazz-File>         name: _foreign        source: {File :context "src" "world" "foreign" "_foreign.jazz"})
          (<Jazz-File>         name: product         source: {File :context "src" "world" "foreign" "product.jazz"})))
      (<Project-Directory>                           directory: {Directory :context "src" "gaia"}
        (<Project-Directory>                         directory: {Directory :context "src" "gaia" "debugger"}
          (<Jazz-File>         name: stub            source: {File :context "src" "gaia" "debugger" "stub.jazz"})))))))
