;;;
;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                   name: processor      description-file: {File :context ".package"}
    (<Project-Directory>                            directory: {Directory :context "src"}
      (<Project-Directory>                          directory: {Directory :context "src" "world"}
        (<Project-Directory>                        directory: {Directory :context "src" "world" "processor"}
          (<Project-Directory>                      directory: {Directory :context "src" "world" "processor" "remote"}
            (<Jazz-File>       name: _remote        source: {File :context "src" "world" "processor" "remote" "_remote.jazz"})
            (<Jazz-File>       name: implementation source: {File :context "src" "world" "processor" "remote" "implementation.jazz"}))
          (<Jazz-File>         name: _processor     source: {File :context "src" "world" "processor" "_processor.jazz"})))))))
