(module world.worker.remote.implementation jazz


(import (world.worker))


(class World-Remote-Worker extends Object
  
  
  (method public (process-setup crashes cache optimize? debug? repositories)
    (worker-process-setup crashes cache optimize? debug? repositories))
  
  
  (method public (process-repositories repositories)
    (worker-process-repositories repositories))
  
  
  (method public (process-unit mode feedback? time? unit-name)
    (worker-process-unit mode feedback? time? unit-name))
  
  
  (method public (process-script mode path)
    (worker-process-script mode path))
  
  
  (method public (process-evaluate mode feedback? syntax str container line col evaluation-mode)
    (worker-process-evaluate mode feedback? syntax str container line col evaluation-mode))
  
  
  (method public (process-run mode feedback? syntax str container line col evaluation-mode)
    (worker-process-run mode feedback? syntax str container line col evaluation-mode))
  
  
  (method public (process-console unit-name expr)
    (worker-process-console unit-name expr))
  
  
  (method public (process-hide)
    (worker-process-hide))
  
  
  (method public (process-quit)
    (worker-process-quit))))
