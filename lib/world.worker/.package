(package world.worker
  
  (root "src")
  (products (worker (alias world.worker))
            (world.worker (update world.worker
                                  world.worker.appl
                                  world.worker.run
                                  world.worker.settings)
                          (dependencies scheme.core
                                        jazz
                                        jazz.designer
                                        jazz.graphic
                                        jazz.io
                                        jazz.jml
                                        jazz.library ;; jazz.event
                                        jazz.platform
                                        jazz.system
                                        jazz.resources
                                        jazz.ui
                                        jiri
                                        ;; debuggee
                                        jazz.debuggee
                                        jazz.debuggee.jazz
                                        jazz.editor.lisp
                                        jazz.editor.jazz
                                        jazz.jrm
                                        jazz.network
                                        ;; profile
                                        jazz.profile))
            (world-worker (title "Worker")
                          (run world.worker.run)
                          (build (world-worker executable: "worker" resources: "resources"))
                          (dependencies world.worker)))
  (project ".project"))
