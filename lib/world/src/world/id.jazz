(module world.id jazz


(import (world)
        (world.context))


(definition public (register-actor id actor)
  (register-actor-id (current-zone) id actor))


(definition public (unregister-actor id)
  (unregister-actor-id (current-zone) id))


(definition public (registered-actor id)
  (registered-actor-id (current-zone) id)))
