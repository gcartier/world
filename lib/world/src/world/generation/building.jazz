(definition jukebox-playing
  #f)

(definition (jukebox-change x y z)
  (if (not jukebox-playing)
      (jukebox-play x y z)
    (jukebox-stop)
    (jukebox-play x y z)))

(definition (jukebox-toggle x y z)
  (if (not jukebox-playing)
      (jukebox-play x y z)
    (jukebox-stop)))

(definition (jukebox-play x y z)
  (let ((world (current-world))
        (music (current-music))
        ;; for tests easier to ear loud music
        (style "warcraft"))
    (let ((path (random-music music style: style)))
      (if (not path)
          (display-error world "No music found")
        (let ((file (effective-music music path)))
          (let ((sound (autoclose-sound file)))
            (set! jukebox-playing sound)
            (play-3d-sound sound (vertex x y z))))))))

(definition (jukebox-stop)
  (when jukebox-playing
    (unregister-3d-sound jukebox-playing)
    (close jukebox-playing)
    (set! jukebox-playing #f)))


