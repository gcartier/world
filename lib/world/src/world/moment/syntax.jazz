(module world.moment.syntax jazz


;;;
;;;; Info
;;;


(macro public (camerabug-info . rest)
  `(jazz-moment level-info (format "{l detail: :human}" (list ,@rest)) category: 'camerabug function: (lexicalname)))

(macro public (live-info . rest)
  `(jazz-moment level-info (format "{l detail: :human}" (list ,@rest)) category: 'live function: (lexicalname)))

(macro public (audio-info . rest)
  `(jazz-moment level-info (format "{l detail: :human}" (list ,@rest)) category: 'audio function: (lexicalname))))
