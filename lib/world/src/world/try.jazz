(module world.try jazz


(import (world)
        (world.context)
        (world.script.asset)
        (world.scriptum)
        (world.settings)
        (world.task)
        (world.work))


(definition (try descriptor)
  (when (scriptable? (current-zone))
    (let ((code
            #/Script/
            (module toto123 script
            (import (script.syntax (phase syntax))
                    (world.script))
            (declare (proper-tail-calls)
                     (safe))
            (%%instance me
                        (let ((a 2))
                          a)))
            //#)
          (instance (new Asset-Entity)))
      (continuation-capture
        (lambda (exit)
          (parameterize ((%%current-instance instance)
                         ; (current-task task)
                         (current-exit exit))
            (worker-run instance code
              ;; read
              (lambda (data)
                (raise data))
              ;; walk
              (lambda (data)
                (raise data))
              ;; error
              (lambda (data)
                (raise data))
              ;; run
              (lambda ()
                )
              ;; result
              (lambda (result)
                (terminal 'result result)))))))))


(register-product-run 'world-try
  try))
