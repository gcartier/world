;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: world            documentation: {File :context "doc" "world.jml"} description-file: {File :context ".package"}
    (<*>                                            tag-reference: {File :context "world.project"})
    (<Project-Directory>                            directory: {Directory :context "src"}
      (<Project-Directory>                          directory: {Directory :context "src" "world"}
        (<Jazz-File>         name: live             source: {File :context "src" "world" "live.jazz"})
        (<Jazz-File>         name: actor            source: {File :context "src" "world" "actor.jazz"})
        (<Jazz-File>         name: area             source: {File :context "src" "world" "area.jazz"})
        (<Jazz-File>         name: block            source: {File :context "src" "world" "block.jazz"})
        (<Jazz-File>         name: data             source: {File :context "src" "world" "data.jazz"})
        (<Jazz-File>         name: enemy            source: {File :context "src" "world" "enemy.jazz"})
        (<Jazz-File>         name: object           source: {File :context "src" "world" "object.jazz"})
        (<Jazz-File>         name: player           source: {File :context "src" "world" "player.jazz"})
        (<Jazz-File>         name: syntax           source: {File :context "src" "world" "syntax.jazz"})
        (<Jazz-File>         name: zone             source: {File :context "src" "world" "zone.jazz"})
        (<Jazz-File>         name: gadget           source: {File :context "src" "world" "gadget.jazz"})
        (<Jazz-File>         name: sector           source: {File :context "src" "world" "sector.jazz"})
        (<Jazz-File>         name: stub             source: {File :context "src" "world" "stub.jazz"})
        (<Jazz-File>         name: tick             source: {File :context "src" "world" "tick.jazz"})
        (<Jazz-File>         name: appl             source: {File :context "src" "world" "appl.jazz"})
        (<Jazz-File>         name: gravity          source: {File :context "src" "world" "gravity.jazz"})
        (<Jazz-File>         name: procedural       source: {File :context "src" "world" "procedural.jazz"})
        (<Jazz-File>         name: tile             source: {File :context "src" "world" "tile.jazz"})
        (<Jazz-File>         name: view             source: {File :context "src" "world" "view.jazz"})
        (<Jazz-File>         name: element          source: {File :context "src" "world" "element.jazz"})
        (<Jazz-File>         name: sun              source: {File :context "src" "world" "sun.jazz"})
        (<Jazz-File>         name: collision        source: {File :context "src" "world" "collision.jazz"})
        (<Jazz-File>         name: plane            source: {File :context "src" "world" "plane.jazz"})
        (<Jazz-File>         name: polygon          source: {File :context "src" "world" "polygon.jazz"})
        (<Jazz-File>         name: generate         source: {File :context "src" "world" "generate.jazz"})
        (<Jazz-File>         name: dye              source: {File :context "src" "world" "dye.jazz"})
        (<Jazz-File>         name: install          source: {File :context "src" "world" "install.jazz"})
        (<Jazz-File>         name: literals         source: {File :context "src" "world" "literals.jazz"})
        (<Project-Directory>                        directory: {Directory :context "src" "world" "geometry"}
          (<Jazz-File>       name: _geometry        source: {File :context "src" "world" "geometry" "_geometry.jazz"})
          (<Jazz-File>       name: classes          source: {File :context "src" "world" "geometry" "classes.jazz"})
          (<Jazz-File>       name: syntax           source: {File :context "src" "world" "geometry" "syntax.jazz"})
          (<Jazz-File>       name: base             source: {File :context "src" "world" "geometry" "base.jazz"}))
        (<Jazz-File>         name: movement         source: {File :context "src" "world" "movement.jazz"})
        (<Jazz-File>         name: lookat           source: {File :context "src" "world" "lookat.jazz"})
        (<Jazz-File>         name: ray              source: {File :context "src" "world" "ray.jazz"})
        (<Jazz-File>         name: binary           source: {File :context "src" "world" "binary.jazz"})
        (<Project-Directory>                        directory: {Directory :context "src" "world" "external"}
          (<Jazz-File>       name: milkshape        source: {File :context "src" "world" "external" "milkshape.jazz"})
          (<Jazz-File>       name: obj              source: {File :context "src" "world" "external" "obj.jazz"})
          (<Jazz-File>       name: targa            source: {File :context "src" "world" "external" "targa.jazz"})
          (<Jazz-File>       name: minecraft        source: {File :context "src" "world" "external" "minecraft.jazz"})
          (<Jazz-File>       name: _external        source: {File :context "src" "world" "external" "_external.jazz"})
          (<Jazz-File>       name: minecraft-syntax source: {File :context "src" "world" "external" "minecraft-syntax.jazz"}))
        (<Jazz-File>         name: dyes             source: {File :context "src" "world" "dyes.jazz"})
        (<Jazz-File>         name: homogeneous      source: {File :context "src" "world" "homogeneous.jazz"})
        (<Jazz-File>         name: opengl           source: {File :context "src" "world" "opengl.jazz"})
        (<Jazz-File>         name: adjustment       source: {File :context "src" "world" "adjustment.jazz"})
        (<Jazz-File>         name: spell            source: {File :context "src" "world" "spell.jazz"})
        (<Jazz-File>         name: boost            source: {File :context "src" "world" "boost.jazz"})
        (<Jazz-File>         name: serialization    source: {File :context "src" "world" "serialization.jazz"})
        (<Jazz-File>         name: mario            source: {File :context "src" "world" "mario.jazz"})))
    (<Project-Directory>                            directory: {Directory :context "script"}
      (<Jazz-File>           name: run              source: {File :context "script" "run.jazz"}))
    (<Project-Directory>                            directory: {Directory :context "doc"}
      (<Project-File>                               source: {File :context "doc" "world.jml"})))))
