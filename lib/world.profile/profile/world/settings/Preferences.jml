        (<Favorite-Compare-Directories> name: "Backup world"  mode: backup left: {Directory Home "world"} right: {Directory Backup "World"} ignored-extensions: ("exe" "pdb" "opt" "lib" "exp" "ilk" "ncb" "suo" "user" "plg" "i" "obj" "o" "o1" "o2" "o3" "o4" "o5" "o6" "a" "dll" "jscm" "pch" "idb" "tdt" "lnk" "crash" "dmp" "stack" "spr") ignored-nodes: ("Crashes" "Debug" "Release" "Work" "bin" "build" "bootstrap" "gambit" ".git") ignored-leaves: ("Session.txt" "vssver.scc" "mssccprj.scc" "Book.ico" "Desktop.ini" "Thumbs.db") check-algorithm: content)
        (<Favorite-Compare-Directories> name: "Jazz Settings" mode: merge left: {Directory "C:"} right: {Directory "C:"} ignored-extensions: ("exe" "pdb" "opt" "lib" "exp" "ilk" "ncb" "suo" "user" "plg" "i" "obj" "o" "o1" "o2" "o3" "o4" "o5" "o6" "a" "dll" "jscm" "pch" "idb" "tdt" "lnk" "crash" "dmp" "stack" "spr") ignored-nodes: ("Crashes" "Debug" "Release" "Work" "bin" "build" "bootstrap" "gambit" ".git") ignored-leaves: ("Session.txt" "vssver.scc" "mssccprj.scc" "Book.ico" "Desktop.ini" "Thumbs.db") check-algorithm: content))
      (<preferences-manager~>                                 position: {Point 131 169} size: {Dimension 1251 461}))
