      (<Favorite-Configuration>                               shortcut: #\V target: {File Profile "settings" "Favorites.jml"})
      (<Favorite-Configuration>                               shortcut: #\W target: {File Profile "settings" "Workbench.jml"})
      (<Favorite-Configuration>                               shortcut: #\X target: {File Profile "settings" "Preferences.jml"})
