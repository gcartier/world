(module world.server.client jazz


(import (world.commands))


(class Server-Client extends Object
  
  
  (slot id                           getter generate)
  (slot client                       accessors generate)
  (slot name                         getter generate)
  (slot alive          initialize #f accessors generate)
  (slot exception      initialize #f accessors generate)
  (slot exception-time initialize #f accessors generate)
  
  
  (method override (initialize id client name)
    (set! id~self id)
    (set! client~self client)
    (set! name~self name))))
