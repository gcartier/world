(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: together description-file: {File :context ".package"}
    (<Project-Directory>                    directory: {Directory :context "src"}
      (<Project-Directory>                  directory: {Directory :context "src" "world"}
        (<Project-Directory>                directory: {Directory :context "src" "world" "together"}
          (<Jazz-File>       name: appl     source: {File :context "src" "world" "together" "appl.jazz"})
          (<Jazz-File>       name: view     source: {File :context "src" "world" "together" "view.jazz"})))))))
